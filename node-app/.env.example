# Server configuration
PORT=3000
NODE_ENV=development

# MongoDB connection
MONGODB_URI=mongodb://localhost:27017/kafka-app

# JWT authentication
JWT_SECRET=ABCD
JWT_EXPIRE=24h

# Kafka configuration
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=api-service
KAFKA_CONSUMER_GROUP=api-service-consumer
KAFKA_SSL=false
KAFKA_SASL=false
KAFKA_SASL_MECHANISM=plain
KAFKA_SASL_USERNAME=
KAFKA_SASL_PASSWORD=

# CORS settings
CORS_ORIGIN=http://localhost:4200

# Logging
LOG_LEVEL=info